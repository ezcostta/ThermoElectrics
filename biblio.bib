@article{freire1,
  AUTHOR = {Freire, H. J. P. and Egues, J. C.},
  TITLE = {Shubnikov-de Haas oscillations in digital magnetic heterostructures},
  YEAR = 2004,
  JOURNAL = {Brazilian Journal of Physics},
  VOLUME = 34,
  NUMBER = 2B,
  PAGES = {614 - 616},
  LANGUAGE = English,
  ISSN = {0103-9733},
  ABSTRACT = {In this paper we theoretically investigate the magnetic-field and temperature dependence of the Shubnikov-de Haas oscillations in group II-VI modulation-doped Digital Magnetic Heterostructures. We self-consistently solve the effective-mass Schrodinger equation within the Hartree approximation and calculate the electronic structure and the magneto-transport properties. Our results show i) a shift of the Shubnikov-de Haas minima to lower magnetic fields with increasing temperature, and ii) an anomalous oscillation which develops when two opposite Landau levels cross near the Fermi energy. Both of these are consistent with recent magneto-tran sport measurements in such heterostructures [R. Knobel et al., Phys. Rev. B 65, 235327 (2002)].},
  DATE = JUN
}

